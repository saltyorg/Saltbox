---
backup:
  cron:
    cron_time: weekly
  local:
    destination: /mnt/local/Backups/Saltbox
    enable: true
  misc:
    snapshot: true
  rclone:
    destination: google:/Backups/Saltbox
    enable: true
    template: google
  restore_service:
    pass:
    user:
  rsync:
    destination: rsync://somehost.com/Backups/Saltbox
    enable: false
    port: 22
